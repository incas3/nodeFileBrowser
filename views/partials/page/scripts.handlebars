<script src="/scripts/bootstrap.min.js"></script>
<script charset="utf-8">
function delete_file(path) {
  if (window.confirm("Are you sure?")) {
    $.ajax({
      url: path,
      type: 'DELETE',
      success: function(result) {
          // Do something with the result
          location.reload();
      }
    });

  }
}
</script>
